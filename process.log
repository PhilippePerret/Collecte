09/04 11:06:06 data_items de :notes : {:created_at=>1491721408, :film_id=>"Everest2016", :items=>{1=>{:id=>1, :scene_id=>nil, :content=>{:raw=>"Cette scène fonctionne avec la scène où [PERSO#rob] va finalement accepter de conduire [PERSO#doug] jusqu'au sommet, pour sa perte.", :only_str=>"Cette scène fonctionne avec la scène où [PERSO#rob] va finalement accepter de conduire [PERSO#doug] jusqu'au sommet, pour sa perte.", :personnages_ids=>["rob", "doug"], :notes_ids=>[], :brins_ids=>[], :scene_id=>nil, :scenes_ids=>nil, :horloge=>nil}}, 2=>{:id=>2, :scene_id=>nil, :content=>{:raw=>"Ce ne sont pas des fanions de foire mais des drapeaux de prière.", :only_str=>"Ce ne sont pas des fanions de foire mais des drapeaux de prière.", :personnages_ids=>[], :notes_ids=>[], :brins_ids=>[], :scene_id=>nil, :scenes_ids=>nil, :horloge=>nil}}, 3=>{:id=>3, :scene_id=>nil, :content=>{:raw=>"On apprend que [PERSO#rob] a perdu un de ses sherpas qui a dû être redescendu.", :only_str=>"On apprend que [PERSO#rob] a perdu un de ses sherpas qui a dû être redescendu.", :personnages_ids=>["rob"], :notes_ids=>[], :brins_ids=>[], :scene_id=>nil, :scenes_ids=>nil, :horloge=>nil}}, 21=>{:id=>21, :scene_id=>nil, :content=>{:raw=>"Indirectement, en mentionnant « Imax », [PERSO#rob] fait référence à l'équipe qui sera la cause du drame (les Sud-Africains) et l'équipe qui permettra de sauver de nombreux grimpeurs, Imax, conduite par l'alpiniste-réalisateur David Breashears qui interviendra activement dans tout le dénouement.", :only_str=>"Indirectement, en mentionnant « Imax », [PERSO#rob] fait référence à l'équipe qui sera la cause du drame (les Sud-Africains) et l'équipe qui permettra de sauver de nombreux grimpeurs, Imax, conduite par l'alpiniste-réalisateur David Breashears qui interviendra activement dans tout le dénouement.", :personnages_ids=>["rob"], :notes_ids=>[], :brins_ids=>[], :scene_id=>nil, :scenes_ids=>nil, :horloge=>nil}}, 4=>{:id=>4, :scene_id=>nil, :content=>{:raw=>"C'est [PERSO#rob] qui explique la chose sous la tente d'Adventure Consultants, mais on voit les images.", :only_str=>"C'est [PERSO#rob] qui explique la chose sous la tente d'Adventure Consultants, mais on voit les images.", :personnages_ids=>["rob"], :notes_ids=>[], :brins_ids=>[], :scene_id=>nil, :scenes_ids=>nil, :horloge=>nil}}, 5=>{:id=>5, :scene_id=>nil, :content=>{:raw=>"Cette scène constitue le PIVOT 1. Il est en relation directe avec la problématique du film, la commercialisation des ascensions de l'Everest.", :only_str=>"Cette scène constitue le PIVOT 1. Il est en relation directe avec la problématique du film, la commercialisation des ascensions de l'Everest.", :personnages_ids=>[], :notes_ids=>[], :brins_ids=>[], :scene_id=>nil, :scenes_ids=>nil, :horloge=>nil}}, 6=>{:id=>6, :scene_id=>nil, :content=>{:raw=>"C'est une belle ironie dans le sens où c'est justement la non utilisation d'oxygène d'[PERSO#anatoli] qui lui permettra de sauver plusieurs vies.", :only_str=>"C'est une belle ironie dans le sens où c'est justement la non utilisation d'oxygène d'[PERSO#anatoli] qui lui permettra de sauver plusieurs vies.", :personnages_ids=>["anatoli"], :notes_ids=>[], :brins_ids=>[], :scene_id=>nil, :scenes_ids=>nil, :horloge=>nil}}, 7=>{:id=>7, :scene_id=>nil, :content=>{:raw=>"Ce dernier temps, situé peut avant le premier tiers, peut être lié au deuxième tiers, moment où Doug perd la vie.", :only_str=>"Ce dernier temps, situé peut avant le premier tiers, peut être lié au deuxième tiers, moment où Doug perd la vie.", :personnages_ids=>[], :notes_ids=>[], :brins_ids=>[], :scene_id=>nil, :scenes_ids=>nil, :horloge=>nil}}, 8=>{:id=>8, :scene_id=>nil, :content=>{:raw=>"La scène se passe en parallèle avec [PERSO#rob] au camp de base.", :only_str=>"La scène se passe en parallèle avec [PERSO#rob] au camp de base.", :personnages_ids=>["rob"], :notes_ids=>[], :brins_ids=>[], :scene_id=>nil, :scenes_ids=>nil, :horloge=>nil}}, 9=>{:id=>9, :scene_id=>nil, :content=>{:raw=>"C'est la scène qui met en place le personnage de [PERSO#guy] (10)", :only_str=>"C'est la scène qui met en place le personnage de [PERSO#guy]", :personnages_ids=>["guy"], :notes_ids=>[10], :brins_ids=>[], :scene_id=>nil, :scenes_ids=>nil, :horloge=>nil}}, 10=>{:id=>10, :scene_id=>nil, :content=>{:raw=>"Le vrai [PERSO#guy] a été conseiller technique sur le film.", :only_str=>"Le vrai [PERSO#guy] a été conseiller technique sur le film.", :personnages_ids=>["guy"], :notes_ids=>[], :brins_ids=>[], :scene_id=>nil, :scenes_ids=>nil, :horloge=>nil}}, 11=>{:id=>11, :scene_id=>nil, :content=>{:raw=>"C'est une préparation de la catastrophe dans le sens où dans la scène suivante on apprendra que c'est Lopsang qui a les cordages destinés à la crête sud-est.", :only_str=>"C'est une préparation de la catastrophe dans le sens où dans la scène suivante on apprendra que c'est Lopsang qui a les cordages destinés à la crête sud-est.", :personnages_ids=>[], :notes_ids=>[], :brins_ids=>[], :scene_id=>nil, :scenes_ids=>nil, :horloge=>nil}}, 12=>{:id=>12, :scene_id=>nil, :content=>{:raw=>"Cette scène explique bien les enjeux commerciaux qui peuvent pousser les guides à l'imprudence. Le business n'est possible que si des clients parviennent au sommet et si les entreprises comme Adventure Consultants bénéficient d'une bonne publicité.", :only_str=>"Cette scène explique bien les enjeux commerciaux qui peuvent pousser les guides à l'imprudence. Le business n'est possible que si des clients parviennent au sommet et si les entreprises comme Adventure Consultants bénéficient d'une bonne publicité.", :personnages_ids=>[], :notes_ids=>[], :brins_ids=>[], :scene_id=>nil, :scenes_ids=>nil, :horloge=>nil}}, 13=>{:id=>13, :scene_id=>nil, :content=>{:raw=>"C'est une nouvelle mise en place de la catastrophe dans le sens où [PERSO#beck] aurait mieux fait de descendre avec les trois clients.", :only_str=>"C'est une nouvelle mise en place de la catastrophe dans le sens où [PERSO#beck] aurait mieux fait de descendre avec les trois clients.", :personnages_ids=>["beck"], :notes_ids=>[], :brins_ids=>[], :scene_id=>nil, :scenes_ids=>nil, :horloge=>nil}}, 14=>{:id=>14, :scene_id=>nil, :content=>{:raw=>"Il ne s'agit pas du sommet Sud, où il y a effectivement 4 bouteilles d'oxygène laissées par Ang Dorjee, qu'Harold finira par trouver.", :only_str=>"Il ne s'agit pas du sommet Sud, où il y a effectivement 4 bouteilles d'oxygène laissées par Ang Dorjee, qu'Harold finira par trouver.", :personnages_ids=>[], :notes_ids=>[], :brins_ids=>[], :scene_id=>nil, :scenes_ids=>nil, :horloge=>nil}}, 15=>{:id=>15, :scene_id=>nil, :content=>{:raw=>"C'est vraiment l'incident de trop, celui qui conduira à la mort de [PERSO#rob] et de [PERSO#doug]. C'est aussi, donc, ce qui empêche le film d'être un mélodrame où seule la météo serait responsable du drame.", :only_str=>"C'est vraiment l'incident de trop, celui qui conduira à la mort de [PERSO#rob] et de [PERSO#doug]. C'est aussi, donc, ce qui empêche le film d'être un mélodrame où seule la météo serait responsable du drame.", :personnages_ids=>["rob", "doug"], :notes_ids=>[], :brins_ids=>[], :scene_id=>nil, :scenes_ids=>nil, :horloge=>nil}}, 16=>{:id=>16, :scene_id=>nil, :content=>{:raw=>"C'est la mort de [PERSO#doug], le début du drame. On se trouve après les deux tiers du film. Un peu avant le premier tiers, Doug expliquait les raisons pour lesquels il devait gravir l'Everest.", :only_str=>"C'est la mort de [PERSO#doug], le début du drame. On se trouve après les deux tiers du film. Un peu avant le premier tiers, Doug expliquait les raisons pour lesquels il devait gravir l'Everest.", :personnages_ids=>["doug"], :notes_ids=>[], :brins_ids=>[], :scene_id=>nil, :scenes_ids=>nil, :horloge=>nil}}, 17=>{:id=>17, :scene_id=>nil, :content=>{:raw=>"On pourrait voir aussi le début du dénouement avec cette mort qui sera la première.", :only_str=>"On pourrait voir aussi le début du dénouement avec cette mort qui sera la première.", :personnages_ids=>[], :notes_ids=>[], :brins_ids=>[], :scene_id=>nil, :scenes_ids=>nil, :horloge=>nil}}, 18=>{:id=>18, :scene_id=>nil, :content=>{:raw=>"Un plan qui sert soi à marquer un changement de partie (entrée dans le dénouement qui devrait se trouver à 1:25:47) soit à jouer une ellipse temporelle.", :only_str=>"Un plan qui sert soi à marquer un changement de partie (entrée dans le dénouement qui devrait se trouver à 1:25:47) soit à jouer une ellipse temporelle.", :personnages_ids=>[], :notes_ids=>[], :brins_ids=>[], :scene_id=>nil, :scenes_ids=>nil, :horloge=>nil}}, 19=>{:id=>19, :scene_id=>nil, :content=>{:raw=>"Ce temps sert de PIVOT 2. Il fait basculer dans le dénouement, en laissant croire qu'un sauvetage va être possible grâce au retournement des forces : alors que le nombre d'équipes était l'handicap, la source du conflit et le drame, ici, il veut devenir une force positive qui peut sauver les grimpeurs. Ça le sera, mais seulement dans une certaine mesure.", :only_str=>"Ce temps sert de PIVOT 2. Il fait basculer dans le dénouement, en laissant croire qu'un sauvetage va être possible grâce au retournement des forces : alors que le nombre d'équipes était l'handicap, la source du conflit et le drame, ici, il veut devenir une force positive qui peut sauver les grimpeurs. Ça le sera, mais seulement dans une certaine mesure.", :personnages_ids=>[], :notes_ids=>[], :brins_ids=>[], :scene_id=>nil, :scenes_ids=>nil, :horloge=>nil}}, 20=>{:id=>20, :scene_id=>nil, :content=>{:raw=>"David Breashears avait 38 ans et se trouvait là pour tourner un documentaire Imax. Il est donc fait indirectement mention de lui au début du film quand, arrivant au camp de base, [PERSO#rob] mentionne l'équipe Imax.", :only_str=>"David Breashears avait 38 ans et se trouvait là pour tourner un documentaire Imax. Il est donc fait indirectement mention de lui au début du film quand, arrivant au camp de base, [PERSO#rob] mentionne l'équipe Imax.", :personnages_ids=>["rob"], :notes_ids=>[], :brins_ids=>[], :scene_id=>nil, :scenes_ids=>nil, :horloge=>nil}}, 22=>{:id=>22, :scene_id=>nil, :content=>{:raw=>"C'est le premier mort du dénouement.", :only_str=>"C'est le premier mort du dénouement.", :personnages_ids=>[], :notes_ids=>[], :brins_ids=>[], :scene_id=>nil, :scenes_ids=>nil, :horloge=>nil}}, 23=>{:id=>23, :scene_id=>nil, :content=>{:raw=>"Le « les » du dialogue n'est pas très clair, mais on comprend dans la scène suivante qu'il parle de Yasuko et Beck.", :only_str=>"Le « les » du dialogue n'est pas très clair, mais on comprend dans la scène suivante qu'il parle de Yasuko et Beck.", :personnages_ids=>[], :notes_ids=>[], :brins_ids=>[], :scene_id=>nil, :scenes_ids=>nil, :horloge=>nil}}, 24=>{:id=>24, :scene_id=>nil, :content=>{:raw=>"Mort du protagoniste, pour peu que [PERSO#rob] soit le protagoniste.", :only_str=>"Mort du protagoniste, pour peu que [PERSO#rob] soit le protagoniste.", :personnages_ids=>["rob"], :notes_ids=>[], :brins_ids=>[], :scene_id=>nil, :scenes_ids=>nil, :horloge=>nil}}, 25=>{:id=>25, :scene_id=>nil, :content=>{:raw=>"Avec cette scène de respiration commence la dernière partie du film, dont la séquence centrale sera le sauvetage de [PERSO#beck] après la mort de [PERSO#rob].", :only_str=>"Avec cette scène de respiration commence la dernière partie du film, dont la séquence centrale sera le sauvetage de [PERSO#beck] après la mort de [PERSO#rob].", :personnages_ids=>["beck", "rob"], :notes_ids=>[], :brins_ids=>[], :scene_id=>nil, :scenes_ids=>nil, :horloge=>nil}}, 26=>{:id=>26, :scene_id=>nil, :content=>{:raw=>"Peut-être est-ce pour le protéger des charognards qu'il le recouvre de son sac, comme dans l'histoire réelle avec un autre alpiniste.", :only_str=>"Peut-être est-ce pour le protéger des charognards qu'il le recouvre de son sac, comme dans l'histoire réelle avec un autre alpiniste.", :personnages_ids=>[], :notes_ids=>[], :brins_ids=>[], :scene_id=>nil, :scenes_ids=>nil, :horloge=>nil}}}}
